<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.layout.ColumnConstraints?>
<ScrollPane fx:id="fxml_parent" fitToHeight="true"
            fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            minHeight="768.0" minWidth="1360.0" prefHeight="1200.0" prefWidth="1100.0"
            stylesheets="@/css/invoices_records.css" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="a.a.a.c.c.e.f.a.ETJ">
    <content>
        <AnchorPane styleClass="background">
            <children>
                <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" spacing="10.0">
                    <children>
                        <fx:include fx:id="fxml_include_left_bar" source="/fxml/common/leftBar.fxml"/>
                        <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                            <children>
                                <fx:include fx:id="fxml_include_top_menu"
                                            source="/fxml/common/topMenu.fxml"
                                            styleClass="h1"/>
                                <ScrollPane fitToHeight="true" fitToWidth="true"
                                            styleClass="border-white" AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="2.0"
                                            AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS">
                                    <content>
                                        <VBox styleClass="background-field" VBox.vgrow="ALWAYS" spacing="5.0">
                                            <children>
                                                <HBox>
                                                    <children>
                                                        <fx:include fx:id="fxml_include_period_box"
                                                                    source="/fxml/periodBox.fxml">
                                                            <columnConstraints>
                                                                <ColumnConstraints />
                                                                <ColumnConstraints maxWidth="80.0"
                                                                                   minWidth="80.0"/>
                                                                <ColumnConstraints maxWidth="100.0"
                                                                                   minWidth="100.0"/>
                                                                <ColumnConstraints/>
                                                            </columnConstraints>

                                                        </fx:include>
                                                    </children>
                                                </HBox>
                                                <TabPane fx:id="fxml_tab_pane" tabClosingPolicy="UNAVAILABLE"
                                                         VBox.vgrow="ALWAYS">
                                                    <tabs>
                                                        <Tab styleClass="btn-tab"
                                                             text="%micro.process.invoices_records.salesTabTitle">
                                                            <content>
                                                                <VBox styleClass="background">
                                                                    <children>
                                                                        <TableView
                                                                                fx:id="fxml_vat_invoice_record_sales_table"
                                                                                VBox.vgrow="ALWAYS">
                                                                          <!--  w kodzie, bo sie zmienia
                                                                          <placeholder>
                                                                                <Label text="%micro.process.invoices_records.empty"/>
                                                                            </placeholder>-->
                                                                            <columnResizePolicy>
                                                                                <TableView
                                                                                        fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                                            </columnResizePolicy>
                                                                            <VBox.margin>
                                                                                <Insets left="5.0" right="5.0"
                                                                                        top="5.0"/>
                                                                            </VBox.margin>
                                                                        </TableView>
                                                                        <AnchorPane>
                                                                            <children>
                                                                                <TableView
                                                                                        fx:id="fxml_vat_invoice_record_sales_summary_table"
                                                                                        disable="true" maxHeight="40.0"
                                                                                        minHeight="40.0"
                                                                                        prefHeight="40.0"
                                                                                        styleClass="noheader"
                                                                                        AnchorPane.bottomAnchor="0.0"
                                                                                        AnchorPane.leftAnchor="5.0"
                                                                                        AnchorPane.rightAnchor="5.0"
                                                                                        AnchorPane.topAnchor="0.0">
                                                                                    <columns></columns>
                                                                                    <columnResizePolicy>
                                                                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                                                    </columnResizePolicy>
                                                                                </TableView>
                                                                            </children>
                                                                        </AnchorPane>
                                                                    </children>
                                                                </VBox>
                                                            </content>
                                                        </Tab>
                                                        <Tab styleClass="btn-tab"
                                                             text="%micro.process.invoices_records.purchasesTabTitle">
                                                            <content>
                                                                <VBox styleClass="background">
                                                                    <children>
                                                                        <TableView
                                                                                fx:id="fxml_vat_invoice_record_purchases_table"
                                                                                VBox.vgrow="ALWAYS">
                                                                            <!--
                                                                            w kodzie, bo sie zmienia
                                                                            <placeholder>
                                                                                <Label text="%micro.process.invoices_records.empty"/>
                                                                            </placeholder>-->
                                                                            <columnResizePolicy>
                                                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                                            </columnResizePolicy>
                                                                        </TableView>
                                                                        <AnchorPane styleClass="background">
                                                                            <children>
                                                                                <TableView
                                                                                        fx:id="fxml_vat_invoice_record_purchases_summary_table"
                                                                                        disable="true" maxHeight="40.0"
                                                                                        minHeight="40.0"
                                                                                        prefHeight="40.0"
                                                                                        styleClass="noheader"
                                                                                        AnchorPane.bottomAnchor="0.0"
                                                                                        AnchorPane.leftAnchor="0.0"
                                                                                        AnchorPane.rightAnchor="0.0"
                                                                                        AnchorPane.topAnchor="0.0">
                                                                                    <columns></columns>
                                                                                    <columnResizePolicy>
                                                                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                                                    </columnResizePolicy>
                                                                                    <VBox.margin>
                                                                                        <Insets left="5.0" right="5.0"/>
                                                                                    </VBox.margin>
                                                                                </TableView>
                                                                            </children>
                                                                        </AnchorPane>
                                                                    </children>
                                                                    <padding>
                                                                        <Insets left="5.0" right="5.0" top="5.0"/>
                                                                    </padding>
                                                                </VBox>
                                                            </content>
                                                        </Tab>
                                                    </tabs>
                                                <!--    <VBox.margin>
                                                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                                                    </VBox.margin>-->
                                                </TabPane>
                                                <VBox AnchorPane.leftAnchor="0.0"
                                                      AnchorPane.rightAnchor="0.0">
                                                    <children>
                                                        <VBox fx:id="fxml_vat_invoice_record_key"
                                                              minHeight="50.0">
                                                            <VBox.margin>
                                                                <Insets left="10.0"/>
                                                            </VBox.margin>
                                                        </VBox>
                                                    </children>
                                                </VBox>
                                            </children>
                                            <padding>
												<Insets bottom="12.0" top="12.0" left="12.0" right="12.0"/>
                                            </padding>
                                        </VBox>
                                    </content>
                                    <padding>
	                               <Insets right="5.0" />
	                            	</padding>
                                </ScrollPane>
                                <HBox alignment="BOTTOM_RIGHT">
                                    <children>
                                        <HBox alignment="BOTTOM_LEFT" >
                                            <children>
                                                <Text fx:id="fxml_textVersions" strokeType="OUTSIDE"
                                                      strokeWidth="0.0" text="Text">
                                                </Text>
                                            </children>
                                        </HBox>
                                        <HBox alignment="BOTTOM_RIGHT"
                                              spacing="10.0" HBox.hgrow="ALWAYS">
                                            <children>
                                                <Button fx:id="fxml_button_print" alignment="CENTER"
                                                        contentDisplay="CENTER" mnemonicParsing="false"
                                                        onAction="#fxml_handleButton_print"
                                                        text="%micro.process.button.Print">
                                                    <styleClass>
                                                        <String fx:value="btn"/>
                                                        <String fx:value="btn-primary"/>
                                                    </styleClass>
                                                </Button>
                                            </children>
                                        </HBox>
                                    </children>
                                    <VBox.margin>
	                               		<Insets bottom="12.0" right="12.0" />
	                            	</VBox.margin>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </content>
</ScrollPane>
