<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.ColumnConstraints?>
<AnchorPane maxHeight="220.0" maxWidth="400.0" minHeight="220.0" minWidth="400.0" prefHeight="220.0" prefWidth="400.0"
            stylesheets="@/css/cash_register_new.css" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="a.a.a.c.c.e.k.a.EVA">
    <children>
        <BorderPane styleClass="background-field" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" maxHeight="150.0" maxWidth="370.0" minHeight="150.0" minWidth="370.0" prefHeight="150.0" prefWidth="370.0" >
            <top>
                <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label styleClass="h2" text="%micro.process.cash_register_new.DialogAddNewElement.DialogName"/>
                    </children>
                    <padding>
                        <Insets bottom="5.0" />
                    </padding>
                </HBox>
            </top>
            <center>
                <GridPane alignment="CENTER" hgap="10.0" VBox.vgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="ALWAYS"/>
                        <ColumnConstraints/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="46.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="46.0" vgrow="SOMETIMES" />
                     <!--   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" /> -->
                        <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                       <fx:include fx:id="fxml_item_net" allowNegative="true"
                                    label="%micro.process.cash_register_new.Net" required="true"
                                    maxLength="15" maximumFractionDigits="2" minimumFractionDigits="2"
                                    source="/fxml/numberBox.fxml" GridPane.columnIndex="0" GridPane.columnSpan="2"
                                    GridPane.rowIndex="0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="170.0" prefWidth="170.0" />
                                <ColumnConstraints hgrow="ALWAYS" />
                            </columnConstraints>
                            <rowConstraints>
                            	<RowConstraints maxHeight="0.0" minHeight="0.0" vgrow="ALWAYS" />
                                <RowConstraints maxHeight="35.0" minHeight="35.0" vgrow="ALWAYS" />
                                <RowConstraints maxHeight="16.0" minHeight="16.0" vgrow="ALWAYS" />
                            </rowConstraints>
                        </fx:include>

                        <fx:include fx:id="fxml_item_tax_rate"
                                    label="%micro.process.cash_register_new.VatRate"
                                    required="true" source="/fxml/requiredComboBox.fxml" GridPane.columnIndex="0"
                                    GridPane.columnSpan="2" GridPane.rowIndex="1">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="170.0" prefWidth="170.0" />
                                <ColumnConstraints hgrow="ALWAYS" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="35.0" minHeight="35.0" vgrow="ALWAYS" />
                                <RowConstraints maxHeight="16.0" minHeight="16.0" vgrow="ALWAYS" />
                            </rowConstraints>
                        </fx:include>

                        <!-- <fx:include fx:id="fxml_item_vat" allowNegative="true"
                                    label="%micro.process.cash_register_new.VatAmount" required="true"
                                    maxLength="15" maximumFractionDigits="2" minimumFractionDigits="2"
                                    source="/fxml/numberBox.fxml" GridPane.columnIndex="0" GridPane.columnSpan="2"
                                    GridPane.rowIndex="2">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="170.0" prefWidth="170.0" />
                                <ColumnConstraints hgrow="ALWAYS" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="35.0" minHeight="35.0" vgrow="ALWAYS" />
                                <RowConstraints maxHeight="16.0" minHeight="16.0" vgrow="ALWAYS" />
                            </rowConstraints>
                        </fx:include>
 -->
                        <HBox spacing="2.0" GridPane.columnIndex="0"
                              GridPane.columnSpan="2" GridPane.rowIndex="3">
                            <VBox.margin>
                                <Insets left="12.0" />
                            </VBox.margin>
                            <children>
                                <Label styleClass="star" text="%micro.user.requiredField.star" />
                                <Label styleClass="h4" text="%micro.user.requiredField" />
                            </children>
                        </HBox>

                    </children>
                </GridPane>
            </center>
            <bottom>
                <HBox alignment="TOP_RIGHT" prefHeight="30.0" spacing="10.0" BorderPane.alignment="CENTER_RIGHT">
                    <children>
                        <Button fx:id="fxml_button_save" alignment="CENTER" contentDisplay="CENTER" disable="true"
                                mnemonicParsing="false" onAction="#fxml_handleButton_save"
                                text="%micro.process.button.Save">
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <styleClass>
                                <String fx:value="btn"/>
                                <String fx:value="btn-primary"/>
                            </styleClass>
                        </Button>
                        <Button fx:id="fxml_button_cancel" alignment="CENTER" contentDisplay="CENTER"
                                mnemonicParsing="false" onAction="#fxml_handleButton_cancel"
                                text="%micro.process.button.Cancel">
                            <styleClass>
                                <String fx:value="btn"/>
                                <String fx:value="btn-danger"/>
                            </styleClass>
                        </Button>
                    </children>
                </HBox>
            </bottom>
            <padding>
                <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
            </padding>
        </BorderPane>
    </children>
</AnchorPane>
